{
  "name": "blitzr-js-sdk",
  "version": "0.0.0",
  "description": "SDK javascript for blitzr API",
  "main": "blitzr.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "open": "google-chrome-stable ./example/index.html",
    "build": "browserify src/index.js -t babelify -s Blitzr --outfile blitzr.js",
    "package": "npm run build && npm run lint && npm run test && uglifyjs blitzr.js -o blitzr.min.js",
    "lint": "eslint ./src ./test",
    "test": "npm run build && mocha ./test"
  },
  "browser": "./blitzr.min.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "gogs@gogs.variusmediagroup.org:Blitzr/blitzr-js-sdk.git"
  },
  "keywords": [
    "blitzr",
    "API",
    "js",
    "javascript",
    "module",
    "plugin",
    "SDK"
  ],
  "author": "yopadd",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.8.0",
    "mocha": "^2.4.5",
    "uglify-js": "^2.6.2",
    "xmlhttprequest": "^1.8.0"
  },
}
