{
  "name": "blitzr-js-sdk",
  "version": "1.0.0",
  "description": "SDK javascript for blitzr API",
  "main": "blitzr.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "browserify src/index.js -t babelify -s Blitzr --outfile blitzr.js",
    "package": "eslint --quiet ./src && npm run build && mocha --compilers js:babel-core/register --reporter='dot' ./test && uglifyjs blitzr.js -o blitzr.min.js",
    "lint": "eslint ./src ./test",
    "test": "npm run build && mocha --compilers js:babel-core/register",
    "debug-mocha": "npm run build && iron-node node_modules/mocha/bin/_mocha --compilers js:babel-core/register",
    "prepublish": "npm run package"
  },
  "browser": "./blitzr.min.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "gogs@gogs.variusmediagroup.org:Blitzr/blitzr-js-sdk.git"
  },
  "keywords": [
    "blitzr",
    "API",
    "js",
    "javascript",
    "module",
    "plugin",
    "SDK"
  ],
  "author": "yopadd",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.8.0",
    "jsdom": "^9.1.0",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "sinon": "^1.17.3",
    "uglify-js": "^2.6.2"
  }
}
