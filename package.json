{
  "name": "blitzr-js-sdk",
  "version": "0.0.0",
  "description": "SDK javascript for blitzr API",
  "main": "blitzr.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "open": "google-chrome-stable ./example/index.html",
    "build": "browserify src/index.js -t babelify -s Blitzr --outfile blitzr.js && uglifyjs blitzr.js -o blitzr.min.js",
    "watch": "babel src/index.js --watch --presets es2015 --out-file blitzr.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browser": "./dist/blitzr.js",
  "babel": {
      "presets": ["es2015"]
  },
  "repository": {
    "type": "git",
    "url": "gogs@gogs.variusmediagroup.org:Blitzr/blitzr-js-sdk.git"
  },
  "keywords": [
    "blitzr",
    "API",
    "js",
    "javascript",
    "module",
    "plugin",
    "SDK"
  ],
  "author": "yopadd",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "uglify-js": "^2.6.2"
  }
}
